................................//jquery inarray function to check element is in array or not.......................

        var input = prompt("What state are you in?");
        var lower = input.toLowerCase();
        var categories = [ 
            "ma", 
            "ny", 
            "ct", 
            "ri"
        ];
        var found = $.inArray(lower, categories);
        if (found > -1) {
            alert("Cool!");
            }
        else {
            alert("Oh no!");
            }


................................//jquery inarray function to check element is in array or not.......................
        var assets_sel=["ma","ny", "ct", "ri"];

            for (var keys in assets_sel) {
                        if (assets_sel[keys] == jsondata[key].id) {
                             sucess="true";  break;
                        }
                        else{
                            sucess="false";
                        }
                    }




................................//jquery variable selector.......................
                var assets_sel= [];
              for(var m=5;m>=0;m--){

                    var select_assets=$("#assets__"+m).val();
                    assets_sel.push(select_assets);

                }
                var y =  (assets_sel.length>0);

